<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test7weeks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Test7weeks">

        <activity android:name=".MainActivity">
            android:label="@string/title_activity_main"
        </activity>

        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.Test7weeks">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"

            android:theme="@style/Theme.Test7weeks">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <activity android:name=".CreateUserActivity">
            android:label="@string/title_activity_createuser"
        </activity>


        <activity android:name=".EEGSignalsActivity">
            android:label="@string/title_activity_eegsignals"
            android:theme="@style/Theme.Test7weeks">
        </activity>


        <activity android:name=".PainStateActivity">
            android:label="@string/title_activity_painstate"
        </activity>

        <activity android:name=".NoPainStateActivity">
            android:label="@string/title_activity_nopainstate"
        </activity>

        <activity android:name=".StateNotFoundActivity">
            android:label="@string/title_activity_statenotfound"
        </activity>



    </application>

</manifest>


